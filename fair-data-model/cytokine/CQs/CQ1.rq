# QC1 â€“ What are all the samples (and its annotations) that were collected from a patient?

# LUMC prefixes
PREFIX rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd:     <http://www.w3.org/2001/XMLSchema#>
PREFIX owl:     <http://www.w3.org/2002/07/owl#>
PREFIX dct:     <http://purl.org/dc/terms/>
PREFIX obo:     <http://purl.obolibrary.org/obo/>
PREFIX ncit:    <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#>
PREFIX sio:     <http://semanticscience.org/resource/>
PREFIX prov:    <http://http://http://www.w3.org/ns/prov#>
PREFIX bc:      <https://rdf.biosemantics.org/resources/beat-covid/>
PREFIX bco:     <http://purl.org/beat-covid/cytokines-semantic-model.owl#>

SELECT ?patient_id ?sample_id ?organ ?sampling_process_date_value WHERE {
    ?person a sio:SIO_000498;
              sio:SIO_000228 ?person_donor_role .

    ?person_donor_role a obo:NCIT_C164796 . # patient role (http://semanticscience.org/resource/SIO_000715)

    ?patient_id a bco:clinical_id ;
                  obo:IAO_0000219 ?person_donor_role .

    ?sample a sio:SIO_001050 ;
              sio:SIO_000628 ?organ .

    ?organ a sio:SIO_010003 ;
          obo:BFO_0000050 ?person_ . # not belong to the same person?

    ?sample_id a bco:record_id ;
                 sio:SIO_000672 ?sample .

    ?sampling_process_ a sio:SIO_001049 ;
                       sio:SIO_000291 ?organ_ ; # not targets the same organ ?
                       sio:SIO_000230 ?person_ ; # not has input from the same person ?
                       sio:SIO_000229 ?sample_ ; # not has input from the same person ?

                       # has quality http://semanticscience.org/resource/SIO_000217
                       sio:SIO_000008 ?sampling_process_date_ .

    ?sampling_process_date_ a obo:NCIT_C25164 ;
                dct:date  ?sampling_process_date_value . # Add a line to get value
}
