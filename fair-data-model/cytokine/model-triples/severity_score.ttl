@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix obo:     <http://purl.obolibrary.org/obo/> .
@prefix sio:     <http://semanticscience.org/resource/> .
@prefix om: 	 <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix swo:	 <http://www.ebi.ac.uk/swo/> .
@prefix repr:	 <https://w3id.org/reproduceme#> .

# SEVERITY SCORE
# Entity
bc:person_ a sio:SIO_000498 ;
           sio:SIO_000228 bc:person_patient_role_ .

# Role
bc:person_patient_role_ a sio:SIO_000715 ;
                        sio:SIO_000356 bc:severity_score_evaluation_process_ .

#Process
bc:severity_score_evaluation_process_ a obo:NCIT_C25214 ;
									  sio:SIO_000229 bc:severity_score_answer ;
									  sio:SIO:000230 bc:severity_score_measurement_method ;
									  sio:SIO_000217 bc:severity_score_evaluation_process_date .

# Process Answer
bc:severity_score_answer a obo:CMO:0001109 ;
						 sio:SIO:000300 "5"^^xsd:float .

# Process Date
bc:severity_score_evaluation_process_date a obo:NCIT_C25164 ;
										  dct:date "29-01-2020 01:00:31"^^xsd:dateTime .

# Process Method
bc:severity_score_measurement_method a obo:NCIT_C81265 ;
									 om:hasScale bc:severity_score_result_scale ;
									 swo:SWO_0004000 bc:severity_score_measurement_version ;
									 sio:SIO:000300 "Leiden Severity Score"^^xsd:string .

# Scale
bc:severity_score_result_scale a obo:NCIT:C25664 ;
							   obo:LABO_0000118 "0"^^xsd:integer ;
							   obo:LABO_0000119 "1"^^xsd:integer .

# Measurement Method Version
bc:severity_score_measurement_version a sio:SIO_000653 ;
								   sio:SIO:000300 "1.0"^^xsd:float .
